{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "no-use-before-define": ["error", { "variables": false }],
  "env": {
    "browser": true
  },
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": [
      "error",
      "never"
    ],
    "no-confusing-arrow": "off",
    "no-unused-expressions": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "react/jsx-one-expression-per-line": "off"
  },
  "plugins": [
    "react"
  ]
}